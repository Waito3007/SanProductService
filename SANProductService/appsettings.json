{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=SANProductDB;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BackgroundLogServiceConfig": {
    "LogDirectory": "Log",
    "MaxFileSizeBytes": 4194304,
    "FilterLogBySources": {
      "SANProductService": {
        "IgnoreByMethodList": ["HealthCheck", "Ping"],
        "FilterList": [
          {
            "PrototypeList": ["Password", "Token", "SecretKey"],
            "Type": 1,
            "ReplaceBy": "********"
          },
          {
            "PrototypeList": ["PhoneNumber"],
            "Type": 2,
            "Start": 3,
            "End": 3
          }
        ]
      }
    }
  }
}
